{
  "include": [
    "src/**/*"
  ],
  "files": [
    "src/styled.d.ts",
    "src/decs.d.ts"
  ],
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "esnext",
    "strict": true,
    "target": "es6",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}